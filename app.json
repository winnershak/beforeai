{
  "expo": {
    "name": "Bliss Alarm",
    "slug": "bliss-alarm-clock",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true,
      "ios": {
        "soundFile": "./assets/sounds/orkney.caf"
      }
    },
    "ios": {
      "bundleIdentifier": "com.yourusername.blissalarm",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "remote-notification"
        ],
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to your Camera.",
        "NSMicrophoneUsageDescription": "This app uses the microphone for...",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library for...",
        "UIViewControllerBasedStatusBarAppearance": false,
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserNotificationUsageDescription": "We need to send you alarm notifications",
        "UIApplicationSupportsIndirectInputEvents": true,
        "AVAudioSessionCategoryOptions": "AVAudioSessionCategoryOptionMixWithOthers",
        "NSFamilyControlsUsageDescription": "Bliss needs access to Screen Time to help you limit distracting apps during focus time."
      },
      "buildNumber": "3",
      "entitlements": {
        "com.apple.developer.family-controls": true,
        "com.apple.developer.usernotifications.time-sensitive": true,
        "com.apple.developer.usernotifications.critical-alerts": true
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/orkney.caf",
            "./assets/sounds/radar.caf",
            "./assets/sounds/beacon.caf",
            "./assets/sounds/chimes.caf",
            "./assets/sounds/circuit.caf",
            "./assets/sounds/reflection.caf",
            "./assets/sounds/slice.caf",
            "./assets/sounds/quiet-boom.caf",
            "./assets/sounds/completeword.caf",
            "./assets/sounds/tetris.caf",
            "./assets/sounds/frenchcomplete.caf",
            "./assets/sounds/mount.caf",
            "./assets/sounds/achieve.caf",
            "./assets/sounds/beach.caf",
            "./assets/sounds/calm-rain.caf",
            "./assets/sounds/fire.caf",
            "./assets/sounds/forest.caf",
            "./assets/sounds/piano.caf",
            "./assets/sounds/white-noise.caf",
            "./assets/sounds/pink-noise.caf",
            "./assets/sounds/rain.caf",
            "./assets/sounds/space.caf",
            "./assets/sounds/thunder.caf",
            "./assets/sounds/waterfall.caf"
          ]
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera."
        }
      ],
      [
        "expo-background-fetch",
        {
          "startOnBoot": true
        }
      ],
      [
        "expo-sensors",
        {
          "motionPermission": "Allow $(PRODUCT_NAME) to access your device motion."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f07e5c10-0728-48d3-b981-ee3955943523"
      }
    },
    "scheme": "blissalarm"
  }
}
