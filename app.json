{
  "expo": {
    "name": "alarmyclone",
    "slug": "alarmyclone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.winnershak.alarmyclone",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/sounds/orkney.mp3",
            "./assets/sounds/radar.mp3",
            "./assets/sounds/beacon.mp3",
            "./assets/sounds/chimes.mp3",
            "./assets/sounds/circuit.mp3",
            "./assets/sounds/reflection.mp3"
          ]
        }
      ]
    ],
    "android": {
      "package": "com.winnershak.alarmyclone"
    }
  }
}
