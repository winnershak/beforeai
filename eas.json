{
  "cli": {
    "version": ">= 3.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "ios": {
        "buildConfiguration": "Release",
        "image": "latest",
        "credentialsSource": "remote",
        "distribution": "store",
        "autoIncrement": true,
        "entitlements": {
          "com.apple.developer.usernotifications.time-sensitive": true,
          "com.apple.developer.usernotifications.critical-alerts": true
        }
      }
    },
    "production-new": {
      "ios": {
        "buildConfiguration": "Release",
        "image": "latest",
        "credentialsSource": "remote",
        "distribution": "store"
      },
      "env": {
        "APP_BUILD_NUMBER": "3"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "erasyl.shahmardanov@icloud.com",
        "ascAppId": "6742881418",
        "appleTeamId": "8L789T788W"
      }
    }
  }
}
